num1 = int(input(15))
num2 = int(input(0))

soma = num1 + num2
produto = num1 * num2
diferenca = num1 - num2

if num2 != 0:
    quociente = num1 / num2
else:
    quociente = "Indefinido (divisão por zero)"

print(f"Soma: {soma}")
print(f"Produto: {produto}")
print(f"Diferença: {diferenca}")
print(f"Quociente: {quociente}")
